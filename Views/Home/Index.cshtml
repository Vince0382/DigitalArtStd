                            <div class="col-5 flip-box">
                                <div class="text-center shSubFrame">
                                    <div class="shSubFrame-header">Team Spirit</div>
                                    <div><img class="shImg" src="~/images/team.png" /></div>
                                    <div class="details-mask"></div>
                                    <div class="details">
                                        <p>By working as a team</p>
                                        <p>We work together</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
